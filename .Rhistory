data = df[df$year %in% 1970:1990, !names(df) %in% "year"],
mean, na.rm = TRUE)
# 1990-2010
aggregate(. ~ country,
data = df[df$year %in% 1990:2010, !names(df) %in% "year"],
mean, na.rm = TRUE)
